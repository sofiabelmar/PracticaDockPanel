<Window x:Class="PracticaDockPanel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PracticaDockPanel"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>

        <Button DockPanel.Dock="Left"> Boton uno </Button>
        <Button DockPanel.Dock="Left">Boton cinco</Button>
        
        <Button DockPanel.Dock="Right">Boton tres</Button>
        
        <Button DockPanel.Dock="Top">Boton dos</Button>

        <Button DockPanel.Dock="Bottom">Boton cuatro</Button>

        <StackPanel>
        <TextBlock Text="Esto es un texto"/>
        <TextBox/>
        <TextBlock Text="Esto es un texto"/>

        <Button >Boton seis</Button>
        </StackPanel>

    </DockPanel>
</Window>
